{
  "name": "hello_world",
  "packageManager": "yarn@3.2.2",
  "dependencies": {
    "@aztec/bb.js": "alpha",
    "@noir-lang/noir-source-resolver": "1.1.1",
    "@noir-lang/noir_wasm": "0.3.2-fa0e9cff",
    "ethers": "^5.6.9"
  },
  "devDependencies": {
    "@types/node": "^18.11.16",
    "@yarnpkg/sdks": "^3.0.0-rc.33",
    "ts-node": "^10.9.1",
    "typescript": "4.9.3"
  },
  "type": "module",
  "scripts": {
    "proof:simple": "yarn ts-node ./node-scripts/generate_simple_proof",
    "proof:poseidon": "yarn ts-node ./node-scripts/generate_poseidon_proof",
    "sol:simple": "yarn ts-node --esm ./node-scripts/generate_sol_verifier.ts './circuits/simple_circuit/src/main.nr' 'simple'",
    "sol:merkle": "yarn ts-node --esm ./node-scripts/generate_sol_verifier.ts './circuits/merkle_tree/src/main.nr' 'merkle'"
  }
}
